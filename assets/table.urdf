    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.026" radius="0.08"/>
      </geometry>
    </collision>
  </link>

  <joint name="kinova1_table_mount_joint" type="fixed">
    <parent link="table_top"/>
    <child link="kinova1_table_mount"/>
    <origin rpy="0 0 0" xyz="-0.72 0.0 0.013"/>
  </joint>

  <joint name="kinova2_table_mount_joint" type="fixed">
    <parent link="table_top"/>
    <child link="kinova2_table_mount"/>
    <origin rpy="0 0 0" xyz="0.72 0 0.013"/>
  </joint>

</robot>
